<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.androidpn.server.model.Log" 
	table="apn_log">
		<composite-id>
			<key-property name="username" type="java.lang.String">
				<column name="username" />
			</key-property>

			<key-property name="action" type="java.lang.Long">
				<column name="action" />
			</key-property>

<!-- 			<key-property name="time" type="java.util.Timestamp"> -->
<!-- 				<column name="time" /> -->
<!-- 			</key-property> -->
		</composite-id>
	</class>
</hibernate-mapping>